<aside style="margin-left: 2em;" class="toc sidebar" data-title="{{or page.attributes.toctitle 'Contents'}}" data-levels="{{{or page.attributes.toclevels 2}}}">
  <div class="toc-menu"></div>
</aside>

{{#with page.navigation}}
<div class="nav-panel-menu is-active" style="visibility: hidden;" data-panel="menu">
  <nav class="nav-menu">
    {{#with @root.page.componentVersion}}
    <h3 class="title"><a href="{{page.component.name}}/{{page.version}}/{{page.module}}/de/index.html">{{./title}}</a></h3>-
    {{/with}}
{{> nav-tree navigation=this}}
  </nav>
</div>
{{/with}}


